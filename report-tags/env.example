# Report Tags Service Environment Configuration
# Copy this to .env and modify as needed

# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=12345678
DB_NAME=cleanapp

# Server Configuration
PORT=8083

# Logging
RUST_LOG=info

# Tag Following Limit
MAX_TAG_FOLLOWS=200

# RabbitMQ Configuration
AMQP_HOST=localhost
AMQP_PORT=5672
AMQP_USER=guest
AMQP_PASSWORD=guest
RABBITMQ_EXCHANGE=cleanapp
RABBITMQ_QUEUE=report-tags
RABBITMQ_RAW_REPORT_ROUTING_KEY=report.raw
RABBITMQ_TAG_EVENT_ROUTING_KEY=tag.added

# Optional: Redis URL for future caching
# REDIS_URL=redis://localhost:6379
